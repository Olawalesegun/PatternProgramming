package pattern_Programming;

public class printL {
	public static void main(String[] args) {
		int n = 5;
		for (int i = 0; i < n; i++) {
			
			 for(int j = 0; j< n; j++) { if(i == 0 || i == 4 || j ==0 || j==4){
				 if(i<n-1 && i>=0 || j== n-1 && j<=(n-1)/2 && j>=0) { 
					 System.out.print("*"); 
				}
				 else { 
					 System.out.print(" "); 
				}
			 }
			 
			System.out.print(" ");

			for (int j = 0; j < n; j++) {
				if (j == 0 || i == 4) {
					System.out.print("*");
				} else {
					System.out.print(" ");
				}
			}
			System.out.println();
		}
	}
}